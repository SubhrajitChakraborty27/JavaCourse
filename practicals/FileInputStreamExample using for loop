package com;

import java.io.FileInputStream;

class InputStreamEx
{
	public void inputTry()
	{
	try {
		FileInputStream f = new FileInputStream("E://test.txt");
		int i = 0;
		for(;(i= f.read())!= -1;)
		System.out.print((char)i);
		f.close();
	}
	catch (Exception e)
	{
		System.out.println(e);
	}
}
}
public class FileInputEx3 
{

	public static void main(String[] args)
	{
		InputStreamEx r = new InputStreamEx();
		r.inputTry();
	}
}
